{
  "name": "myfoodmap-server",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:nodemon": "nodemon --exec \"npx tsx src/index.ts\" --watch src --ext ts",
    "build": "tsc",
    "postbuild": "npx prisma generate --schema=./shared/prisma/schema.prisma",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "seed": "tsx ./shared/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.8.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  }
}
